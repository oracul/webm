<h1>Listing user_interactions</h1>

<table>
  <thead>
    <tr>
      <th>Owner</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_interactions.each do |user_interaction| %>
      <tr>
        <td><%= User.find(current_user).id %></td>
        <td><%= user_interaction.owner %></td>
        <td><%= link_to 'Show', user_interaction %></td>
        <td><%= link_to 'Edit', edit_user_interaction_path(user_interaction) %></td>
        <td><%= link_to 'Destroy', user_interaction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User interaction', new_user_interaction_path %>
